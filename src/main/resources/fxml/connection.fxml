<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="200.0" prefWidth="180.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ConnectionController">
    <children>
        <TextField fx:id="tfUser" layoutX="16.0" layoutY="34.0" promptText="user"/>
        <TextField fx:id="tfPassword" layoutX="16.0" layoutY="76.0" promptText="password"/>
        <TextField fx:id="tfDbName" layoutX="16.0" layoutY="122.0" promptText="Database Name"/>
        <Button layoutX="111.0" layoutY="168.0" mnemonicParsing="false" onAction="#connect" text="connect"/>
        <Label layoutX="30.0" layoutY="6.0" text="Connection to database"/>
    </children>
</AnchorPane>
